{
    "name": "easymove-api",
    "version": "0.0.1",
    "private": true,
    "description": "",
    "author": "",
    "license": "MIT",
    "scripts": {
        "prebuild": "rimraf dist",
        "build": "nest build",
        "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
        "start": "nest start",
        "start:dev": "nest start --watch",
        "start:debug": "nest start --debug --watch",
        "start:prod": "node dist/main",
        "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
        "test": "cross-env NODE_ENV=test jest --runInBand",
        "test:watch": "cross-env NODE_ENV=test jest --watch --runInBand",
        "test:cov": "cross-env NODE_ENV=test jest --coverage --runInBand",
        "test:debug": "cross-env NODE_ENV=test node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
        "test:e2e": "cross-env NODE_ENV=test jest --config ./test/jest-e2e.json",
        "test:clear": "jest --clearCache",
        "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js",
        "migration:generate": "ts-node node_modules/.bin/typeorm migration:generate",
        "migration:run": "ts-node ./node_modules/typeorm/cli.js migration:run",
        "migration:revert": "ts-node node_modules/.bin/typeorm migration:revert"
    },
    "dependencies": {
        "@nestjs/common": "^7.0.1",
        "@nestjs/config": "^0.4.0",
        "@nestjs/core": "^7.0.1",
        "@nestjs/jwt": "^7.0.0",
        "@nestjs/passport": "^7.0.0",
        "@nestjs/platform-express": "^7.0.1",
        "@nestjs/swagger": "^4.4.0",
        "@nestjs/typeorm": "^7.0.0",
        "bcrypt": "^4.0.0",
        "class-transformer": "^0.2.3",
        "class-validator": "^0.11.0",
        "dotenv": "^8.2.0",
        "jsonwebtoken": "^8.5.1",
        "lodash": "^4.17.15",
        "moment": "^2.24.0",
        "mysql": "^2.18.1",
        "passport": "^0.4.1",
        "passport-jwt": "^4.0.0",
        "passport-local": "^1.0.0",
        "pg": "^8.0.0",
        "reflect-metadata": "^0.1.13",
        "rimraf": "^3.0.0",
        "rxjs": "^6.5.4",
        "swagger-ui-express": "^4.1.3",
        "typeorm": "^0.2.22",
        "uuid": "^7.0.0"
    },
    "devDependencies": {
        "@nestjs/cli": "^7.0.0",
        "@nestjs/schematics": "^7.0.0",
        "@nestjs/testing": "^7.0.1",
        "@types/bcrypt": "^3.0.0",
        "@types/express": "^4.17.2",
        "@types/jest": "25.1.2",
        "@types/jsonwebtoken": "^8.3.7",
        "@types/lodash": "^4.14.149",
        "@types/node": "^13.1.6",
        "@types/passport": "^1.0.2",
        "@types/passport-jwt": "^3.0.3",
        "@types/supertest": "^2.0.8",
        "@types/uuid": "^7.0.0",
        "@typescript-eslint/eslint-plugin": "^2.12.0",
        "@typescript-eslint/parser": "^2.12.0",
        "cross-env": "^7.0.0",
        "eslint": "^6.7.2",
        "eslint-config-prettier": "^6.7.0",
        "eslint-plugin-import": "^2.19.1",
        "jest": "^25.0.0",
        "prettier": "^1.18.2",
        "supertest": "^4.0.2",
        "ts-jest": "25.2.0",
        "ts-loader": "^6.2.1",
        "ts-node": "^8.6.0",
        "tsconfig-paths": "^3.9.0",
        "typescript": "^3.7.4"
    },
    "jest": {
        "moduleFileExtensions": [
            "js",
            "json",
            "ts"
        ],
        "rootDir": "src",
        "testRegex": ".spec.ts$",
        "transform": {
            "^.+\\.(t|j)s$": "ts-jest"
        },
        "coverageDirectory": "../coverage",
        "testEnvironment": "node"
    }
}
